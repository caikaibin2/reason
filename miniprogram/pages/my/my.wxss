.my {
  background-color: white;
}
.my .portrait-box {
  width: 100%;
  height: 400rpx;
}
.my .portrait-box .portrait-img {
  background-color: #f5f6f7;
  margin: 0 auto;
  padding: 90rpx 0;
  text-align: center;
}
.my .portrait-box .portrait-img image {
  width: 130rpx;
  height: 130rpx;
  border-radius: 50%;
}
.my .portrait-box .portrait-img text {
  display: block;
  margin: 0 auto;
  margin-top: 20rpx;
}
.my .portrait-box .business-box {
  width: 100%;
}
.my .portrait-box .business-box .business {
  width: 100%;
  height: 80rpx;
  display: flex;
  line-height: 80rpx;
  background-color: white;
  border-bottom: 2rpx solid silver;
}
.my .portrait-box .business-box .business .text {
  line-height: 80rpx;
  margin-left: 40rpx;
}
.my .portrait-box .business-box .business .img-box {
  width: 80rpx;
  height: 80rpx;
  margin-left: auto;
  margin-right: 40rpx;
}
.my .portrait-box .business-box .business .img-box image {
  text-align: center;
  margin-left: 8px;
  vertical-align: middle;
  width: 50rpx;
  height: 50rpx;
}
